{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "plugin:react-hooks/recommended",
        "standard"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
    },
    "overrides": [
        {
            "files": ["*"],
            "rules": {
                "padded-blocks": "off",
                "react/prop-types": "off",
                "react/no-deprecated": "off"
            }
        }
   ],
    "settings": {
        "react": {
            "version": "detect"
          }
    }
}
